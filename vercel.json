{
  "functions": {
    "backend/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(?!static)(.*)",
      "destination": "/backend/server.js"
    },

    {
      "source": "/(.*)",
      "destination": "/frontend/index.html"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:js|css|png|jpg|gif|ico|svg))",
      "dest": "/frontend/$1"
    }
  ]
}
